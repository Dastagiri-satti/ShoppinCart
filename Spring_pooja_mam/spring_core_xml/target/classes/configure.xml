<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">


	<bean id="mySample" class="xml_based_Configuration.Sample" />


<bean id="myperson" class="xml_based_Configuration.Person">

	<constructor-arg name="name" value="dinakar"></constructor-arg>
	<constructor-arg name="age" value="23"></constructor-arg>
</bean>


<bean id="myStudent" class="xml_based_Configuration.Student">
<property name="name" value="Krishna"></property>
<property name="age" value="23"></property>
<property name="Address" value="kadapa"></property>

</bean>
	






</beans>
